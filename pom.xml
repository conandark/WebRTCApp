	</organization>
	<scm>
		<url>https://mekya@bitbucket.org/ant-media/apps.git</url>
		<connection>https://mekya@bitbucket.org/ant-media/apps.git</connection>
		<developerConnection>mekya@antmedia.io</developerConnection>
	</scm>
</project>
